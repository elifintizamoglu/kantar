<mvc:View
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc">
    <Panel>
        <List
        headerText="Backend Verileri"
        items="{
            path: 'getAllData>/',
            sorter: {
                path: 'id',
                descending: false
            }
        }"
    >
        <items>
            <ObjectListItem
                title="{getAllData>plaka}"
                number="{getAllData>id}"
            >
                <attributes>
                    <ObjectAttribute title="Giriş Ağırlık" text="{getAllData>giris_agirlik}"/>
                    <ObjectAttribute title="Giriş Saati" text="{getAllData>giris_saati}"/>
                    <ObjectAttribute title="Çıkış Ağırlık" text="{getAllData>cikis_agirlik}"/>
                    <ObjectAttribute title="Çıkış Saati" text="{getAllData>cikis_saati}"/>
                </attributes>
            </ObjectListItem>
        </items>
    </List>
    </Panel>
    
</mvc:View>