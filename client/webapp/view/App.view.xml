<mvc:View
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc">
    <Panel>
        <Table id="myTable" items="{getAllData>/}">
        <!-- Tablo sütunları -->
        <columns>
            <Column>
                <Text text="Plaka"/>
            </Column>
            <Column>
                <Text text="Giriş Ağırlık"/>
            </Column>
            <Column>
                <Text text="Giriş Saati"/>
            </Column>
            <Column>
                <Text text="Çıkış Ağırlık"/>
            </Column>
            <Column>
                <Text text="Çıkış Saati"/>
            </Column>
        </columns>
        <!-- Tablo satırları -->
        <items>
            <ColumnListItem>
                <cells>
                    <Text text="{getAllData>plaka}"/>
                    <Text text="{getAllData>giris_agirlik}"/>
                    <Text text="{getAllData>giris_saati}"/>
                    <Text text="{getAllData>cikis_agirlik}"/>
                    <Text text="{getAllData>cikis_saati}"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>
    </Panel>
    
</mvc:View>